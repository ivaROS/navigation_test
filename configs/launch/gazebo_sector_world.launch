<launch>
  <arg name="world_file" default="$(find nav_configs)/world/sector_world_laser.world"/>
  <arg name="map_file" default="$(find nav_configs)/maps/sector_world.yaml"/>
  <!--<arg name="ground_truth_map_file" default="$(find nav_configs)/maps/ground_truth/sector_world_laser.yaml"/> -->
  <arg name="gui" default="$(optenv GM_PARAM_GAZEBO_GUI true)"/>

  <include file="$(find nav_configs)/launch/gazebo_general.launch">
    <arg name="world_file" value="$(arg world_file)"/>
    <arg name="map_file" value="$(arg map_file)"/>
    <!--<arg name="ground_truth_map_file" value="$(arg ground_truth_map_file)"/> -->
    <arg name="gui" value="$(arg gui)"/>
  </include>

</launch>
